Cell[BoxData[
 RowBox[{
  RowBox[{"$PrePrint", "=", 
   RowBox[{
    RowBox[{"#", "/.", 
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{"Derivative", "[", "id__", "]"}], "[", "f_", "]"}], "[", 
       "args__", "]"}], "\[RuleDelayed]", 
      RowBox[{"TraditionalForm", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"HoldForm", "@", 
          RowBox[{"D", "[", 
           RowBox[{
            RowBox[{"f", "[", "args", "]"}], ",", "#"}], "]"}]}], "&"}], "[", 
        
        RowBox[{"Sequence", "@@", 
         RowBox[{"(", 
          RowBox[{
           RowBox[{"DeleteCases", "[", 
            RowBox[{
             RowBox[{"Transpose", "[", 
              RowBox[{"{", 
               RowBox[{
                RowBox[{"{", "args", "}"}], ",", 
                RowBox[{"{", "id", "}"}]}], "}"}], "]"}], ",", 
             RowBox[{"{", 
              RowBox[{"_", ",", "0"}], "}"}]}], "]"}], "/.", 
           RowBox[{
            RowBox[{"{", 
             RowBox[{"x_", ",", "1"}], "}"}], "\[RuleDelayed]", "x"}]}], 
          ")"}]}], "]"}], "]"}]}]}], "&"}]}], ";"}]], "Input",
